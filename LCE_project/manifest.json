{
    "manifest_version": 2,
    "name": "LCE-kmitl",
    "version": "1.5",
    "description": "change your keyboard Language",
    "icons": {
            "16": "icon/De16x16.png",
            "48": "icon/De48x48.png",
            "128": "icon/De128x128.png"
            },

    "permissions": [
        "contextMenus",
        "tabs",
        "activeTab"
    ],
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "windows": "Alt+C",
            "mac": "Alt+C",
            "chromeos": "Alt+C",
            "linux": "Alt+C"
          }
        }
    },
    "background":{
        "scripts": ["context.js"],
        "persitent": false
    },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "icon/De16x16.png",
            "48": "icon/De48x48.png",
            "128": "icon/De128x128.png"
        }}
}
